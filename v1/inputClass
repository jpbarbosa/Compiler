class gcd_rec {
	public static void main(String[] args) {
		int a = 5, b;
		
		a = 315;
		b = 1932;
		
		System.out.println(gcd(a,b));
	}
	
	
	public static int gcd(int a, int b) {
		if (a == b || b == 0)
			return a;
		else if (a == 0)
			return b;
		else if (a > b)
			return gcd(a % b, b);
/* b < a WARNING: Se estiver na mesma linha que cÃ³digo ou ter tabs ou barras n, dÃ¡ bode, tipo o exemplo original.*/
		else
			return gcd(a, b % a);
	}
}
